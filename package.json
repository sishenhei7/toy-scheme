{
  "name": "toy-scheme",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "format:toml": "taplo format --check '.cargo/*.toml' './packages/**/Cargo.toml' './Cargo.toml'",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint ./packages --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "lint-staged": {
    "*.rs": "rustfmt --edition 2021",
    "*.(js,jsx,cjs,mjs,ts,tsx,cts,mts)": "pnpm run lint",
    "*.toml": "pnpm run format:toml"
  },
  "dependencies": {
    "monaco-editor": "^0.37.0",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@taplo/cli": "^0.5.2",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-dom": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^5.0.0",
    "sass": "^1.60.0",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12",
    "why-is-node-running": "^2.2.2"
  }
}
